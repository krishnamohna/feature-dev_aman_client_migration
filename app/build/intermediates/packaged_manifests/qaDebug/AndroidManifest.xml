            android:name="com.cardio.physician.ui.views.splash.SplashActivity"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.cardio.physician.ui.views.tutorial.TutorialActivity"
            android:screenOrientation="portrait" />
