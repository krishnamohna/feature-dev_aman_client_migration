        <activity android:name="com.cardio.physician.ui.views.illness.IllnessActivity" />

        <provider
            android:name="androidx.core.content.FileProvider"
            android:authorities="com.cardio.physician.qa.fileprovider"
            android:exported="false"
            android:grantUriPermissions="true" >
            <meta-data
                android:name="android.support.FILE_PROVIDER_PATHS"
                android:resource="@xml/file_paths" />
        </provider> <!-- fitbit activities -->
        <service
            android:name="com.cardio.physician.ui.service.SyncHeathDataService"
            android:enabled="true"
            android:exported="true" />

        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />
        <!--
 Optional: Add it to automatically download ML model to device after
  your app is installed.
        -->
        <meta-data
            android:name="com.google.mlkit.vision.DEPENDENCIES"
            android:value="ocr,ica" />

        <!-- for firebase push notificatons -->
        <service
            android:name="com.cardio.physician.ui.service.FirebasePushNotificationService"
            android:exported="false" >
