                <data
                    android:host="fitbit"
                    android:scheme="pocketcardio" />
            </intent-filter>
            -->
        </activity>

        <meta-data
            android:name="com.fitbit.sampleandroidoauth2.CLIENT_ID"
            android:value="23BD79" />
        <meta-data
            android:name="com.fitbit.sampleandroidoauth2.REDIRECT_URL"
            android:value="pocketCardio://fitbit/auth" />

